    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch backend server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Queue",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/VideoService/VideoWorkerAndQueue/videoQueue.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch current server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        }
    ]
}